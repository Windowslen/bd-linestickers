div.line-pack(data-id=startingId)
  div.line-editbar
    span.item
      svg.icon-plus-cross(width=18, height=18, style="opacity:0.6")
        g(fill="none", fill-rule="evenodd")
          path(d="M0 0h18v18H0", stroke-width="2px")
          path(stroke="#FFF", d="M4.5 4.5l9 9", stroke-linecap="round", stroke-width="2px")
          path(stroke="#FFF", d="M13.5 4.5l-9 9", stroke-linecap="round", stroke-width="2px")
    span.item
      svg.icon-edit(width=18, height=18)
        path(fill="#737F8D", fill-rule="evenodd", d="M3 14v-2.5l7.88-7.85c.19-.2.51-.2.71 0l1.76 1.76c.2.2.2.51 0 .71L5.47 14H3zm12 0H7.5l2-2H15v2z")

  div.line-pack-header= title

  div.line-pack-stickers
    - for (let id = startingId; id < startingId + length; id++)
      div.emote-container
        img.emote-icon(src="https://sdl-stickershop.line.naver.jp/stickershop/v1/sticker/" + id + "/android/sticker.png;compress=true")
